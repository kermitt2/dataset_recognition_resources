                  <td align="left" rowspan="1" colspan="1">&#x2212;40</td>
                  <td align="left" rowspan="1" colspan="1">10</td>
                  <td align="left" rowspan="1" colspan="1">&#x2212;</td>
                </tr>
                <tr>
                  <td align="left" rowspan="1" colspan="1"/>
                  <td align="left" rowspan="1" colspan="1">C (mV)</td>
                  <td align="left" rowspan="1" colspan="1">20</td>
