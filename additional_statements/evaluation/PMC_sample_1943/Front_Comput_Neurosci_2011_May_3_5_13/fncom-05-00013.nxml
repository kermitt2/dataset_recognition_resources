                </tr>
                <tr>
                  <td align="left" rowspan="1" colspan="1">
                    <inline-formula>
                      <mml:math id="M19">
                        <mml:mrow>
                          <mml:msub>
                            <mml:mover accent="true">
                              <mml:mi>g</mml:mi>
                              <mml:mo>&#xAF;</mml:mo>
                            </mml:mover>
                            <mml:mrow>
                              <mml:mtext>K(Ca)AP</mml:mtext>
