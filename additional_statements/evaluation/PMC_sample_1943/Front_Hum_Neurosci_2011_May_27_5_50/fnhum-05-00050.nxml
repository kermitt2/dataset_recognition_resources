                <td align="left" rowspan="1" colspan="1"/>
                <td align="left" rowspan="1" colspan="1">Amatuer</td>
                <td align="left" rowspan="1" colspan="1">63.8 (4.3)</td>
                <td align="left" rowspan="1" colspan="1">53.9 (3.6)</td>
                <td align="left" rowspan="1" colspan="1">48.0 (1.5)</td>
                <td align="left" rowspan="1" colspan="1">41.2 (4.1)</td>
