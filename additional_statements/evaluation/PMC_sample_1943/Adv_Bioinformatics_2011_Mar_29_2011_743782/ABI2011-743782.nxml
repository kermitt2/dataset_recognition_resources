                            <mml:mrow>
                              <mml:mtable>
                                <mml:mtr>
                                  <mml:mtd>
                                    <mml:mtext>Number of FP&#x2003;if</mml:mtext>
                                    <mml:mo>&#x2009;</mml:mo>
                                    <mml:mo>&#x2009;</mml:mo>
                                    <mml:mtext>FP</mml:mtext>
                                    <mml:mo>&lt;</mml:mo>
                                    <mml:mi>T</mml:mi>
                                    <mml:mi>P</mml:mi>
                                  </mml:mtd>
                                </mml:mtr>
                                <mml:mtr>
                                  <mml:mtd>
                                    <mml:mtext>Number of TP&#x2003;if</mml:mtext>
                                    <mml:mo>&#x2009;</mml:mo>
                                    <mml:mo>&#x2009;</mml:mo>
                                    <mml:mtext>FP</mml:mtext>
                                    <mml:mo>&gt;</mml:mo>
                                    <mml:mi>T</mml:mi>
                                    <mml:mi>P</mml:mi>
                                  </mml:mtd>
                                </mml:mtr>
                              </mml:mtable>
                            </mml:mrow>
